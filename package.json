    "babel-register": "^6.3.13",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.9.2",
    "q": "^1.4.1",
    "react": "^15.1.0",
    "react-engine": "^3.0.0",
    "react-router": "^2.4.0",
    "request": "^2.72.0",
